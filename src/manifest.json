{
  "manifest_version": 2,
  "name": "Garmin Connect Web Kudo All",
  "version": "0.2",
  "description": "Give kudos to your elite athletes friends in one click一键给你的精英运动员朋友们点赞",
  "default_locale": "en",
  "icons": {
    "48": "icons/icon-48x48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://connect.garmin.com/*",
        "*://connect.garmin.cn/*",
        "*://connectus.garmin.cn/*"
      ],
      "js": [
        "kudoall.js"
      ]
    }
  ]
}